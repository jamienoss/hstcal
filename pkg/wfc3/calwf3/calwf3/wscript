# vim: set syntax=python:

def build(bld):
    t = bld.program(
        name = 'calwf3.e',
        source = 'wf3main.c',
        target = 'calwf3.e',
        use = ['calwf3', 'imphttab', 'wf3cte-fast'] + bld.env.LOCAL_LIBS,
        lib = bld.env.EXTERNAL_LIBS,
        libpath = bld.env.LIBPATH,
        install_path = '${PREFIX}/bin'
        )
